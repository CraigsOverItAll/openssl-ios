module ssl [system] {
    module asn1 {
        header "openssl/asn1.h"
        export *
    }
    module bio {
        header "openssl/bio.h"
        export *
    }
    module bn {
        header "openssl/bn.h"
        export *
    }
    module buffer {
        header "openssl/buffer.h"
        export *
    }
    module crypto {
        header "openssl/crypto.h"
        export *
    }
    module dh {
        header "openssl/dh.h"
        export *
    }
    module dsa {
        header "openssl/dsa.h"
        export *
    }
    module e_os2 {
        header "openssl/e_os2.h"
        export *
    }
    module ec {
        header "openssl/ec.h"
        export *
    }
    module ecdh {
        header "openssl/ecdh.h"
        export *
    }
    module ecdsa {
        header "openssl/ecdsa.h"
        export *
    }
    module evp {
        header "openssl/evp.h"
        export *
    }
    module lhash {
        header "openssl/lhash.h"
        export *
    }
    module obj_mac {
        header "openssl/obj_mac.h"
        export *
