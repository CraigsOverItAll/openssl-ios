    module safestack {
        header "openssl/safestack.h"
        export *
    }
    module sha {
        header "openssl/sha.h"
        export *
    }
    module stack {
        header "openssl/stack.h"
        export *
    }
    module symhacks {
        header "openssl/symhacks.h"
        export *
    }
    module x509 {
        header "openssl/x509.h"
        export *
    }
    module x509_vfy {
        header "openssl/x509_vfy.h"
        export *
    }
    
    export *
